# -----------------------------------------------------
# Fastfetch
# -----------------------------------------------------
if [[ $(tty) == *"pts"* ]]; then
    if command -v fastfetch >/dev/null; then
        fastfetch --config examples/13
    fi
else
    echo
    if command -v hyprctl >/dev/null; then
        echo "Start Hyprland with command Hyprland"
    fi
fi
